# vim: set ft=zsh

# Aliases
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias ls="ls --color=auto"
alias ll="ls -al"
alias l="ll"
alias lg="lazygit"
alias c="cd"
alias cl="clear"
alias n="nvim"
alias cat="bat"
alias mux="tmuxinator"
alias nz="nvim ~/.zshrc"
alias na="nvim ~/.zsh/aliases"
alias nn="nvim ~/.config/nvim/init.lua"
alias ez="exec zsh"
